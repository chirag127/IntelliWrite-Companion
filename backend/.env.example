# Server configuration
PORT=3000
NODE_ENV=development

# CORS settings
ALLOWED_ORIGINS=chrome-extension://YOUR_EXTENSION_ID,http://localhost:3000

# Rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Gemini API configuration
# This is just a placeholder - users will provide their own API key through the extension
# The backend will use the API key provided by the extension in each request
GEMINI_API_MODEL=gemini-2.5-flash-preview-04-17
